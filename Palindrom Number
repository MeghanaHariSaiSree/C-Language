#include<stdio.h>
int main()
{
    int temp=0,rev,num,orgnum;
    printf("Enter a number :");
    scanf("%d",&num);
    orgnum=num;
    while(num>0)
    {
        rev=num%10;
        num=num/10;
        temp=temp*10+rev;
    }
    if(orgnum==temp){
        printf("%d is a Palindrom number",orgnum);
    }
    else
    {
        printf("%d is not a Palindrom number",orgnum);
    }
    return 0;
}
