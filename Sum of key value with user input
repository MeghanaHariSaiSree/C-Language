#include<stdio.h>
#include<conio.h>
int digits(int);
void main()
{
    int arr[30];
    int num;
    printf("Enter how many numbers should be in the array : ");
    scanf("%d",num);
    for(int i=0;i<num;i++)
    {
        printf("Enter number %d : ",i+1);
        scanf("%d",&arr[i]);
    }
    int sum = 0;
    for(int i=0;i<num;i++)
    {
        sum = sum + digits(arr[i]);
    }
    printf("The key value is %d",sum);
}
int digits(int a)
{
    int res =0;
    while(a>0)
    {
        res = res + a%10;
        a = a/10;
    }
    return res;
}
