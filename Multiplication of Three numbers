#include<stdio.h>
int main()
{
    int num1,num2,num3,mul;
    printf("Enter the first number : ");
    scanf("%d",&num1);
    printf("Enter the second number : ");
    scanf("%d",&num2);
    printf("Enter the third number : ");
    scanf("%d",&num3);
    mul=num1*num2*num3;
    printf("multiplication of %d,%d,%d is : %d",num1,num2,num3,mul);
    return 0;
}
