#include<stdio.h>
void main()
{
    char s[50];
    printf("Enter a word : ");
    gets(s);
    int len=strlen(s)-1;
    for(int i=0,j=len;i<j;i++,j--)
    {
        if(s[i]!=s[j])
            {
            printf("%s is not a palindrome",s);
            return;
            }
    }
    printf("%s is a palindrome",s);
}

