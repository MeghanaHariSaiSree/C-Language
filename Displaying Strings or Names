#include<stdio.h>
void main()
{

    char name[10][10];
    int num;
    printf("Enter number of names:");
    scanf("%d",&num);
    for(int i=0;i<num;i++)
    {
       scanf("%s",name[i]);

    }
    printf("The names are as follows........\n");
    for(int i=0;i<num;i++)
    {
       printf("Name%d.%s\n",i+1,name[i]);
    }

}
